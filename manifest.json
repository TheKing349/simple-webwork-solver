{
  "manifest_version": 3,
  "name": "WeBWorK Solver",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "clipboardWrite"
  ],
  "host_permissions": [
    "[URL_HERE]/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}